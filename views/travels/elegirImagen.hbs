<form action="/destinos/travelimagen/{{travelId}}" method="post" id="form1">
    <div class="container">
        <div class="row">
            {{#each imagenes}}
            <div class="col-3">
                {{#if profile}}
                <div class="form-check">
                    <input class="form-check-input" type="radio" name="check{{travelId}}" id="check{{travelId}}" value="{{id}}" checked>
                    <label class="form-check-label" for="check{{travelId}}"></label>
                </div>
                {{else}}
                <div class="form-check">
                    <input class="form-check-input" type="radio" name="check{{travelId}}" id="check{{travelId}}" value="{{id}}">
                    <label class="form-check-label" for="check{{travelId}}"></label>
                </div>
                {{/if}}
                <div class="card">
                    <a href="/destinos/{{id}}"><img src="/imagen/{{path}}" class="card-img-top"
                            height="150px"></a>
                    <div class="card-body">

                    </div>
                </div>

            </div>

            {{/each}}

        </div>
    </div>

    <button id="cambios">Guardar cambios</button>

</form>